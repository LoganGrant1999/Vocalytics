{
  "buildCommand": "pnpm install && pnpm --filter web build",
  "outputDirectory": "packages/web/dist",
  "installCommand": "pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/webhook/:path*",
      "destination": "/api"
    },
    {
      "source": "/healthz",
      "destination": "/api"
    }
  ]
}

{
  "buildCommand": "pnpm install && pnpm --filter @vocalytics/web build",
  "outputDirectory": "packages/web/dist",
  "installCommand": "pnpm install",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/[...path]" },
    { "source": "/((?!api).*)", "destination": "/index.html" }
  ]
}

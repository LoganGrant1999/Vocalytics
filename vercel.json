{
  "buildCommand": "cd ../.. && pnpm --filter @vocalytics/web build && ls -la packages/web/",
  "outputDirectory": "packages/web/dist",
  "installCommand": "pnpm install",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/[...path]" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
